Host *
    Protocol 2
    Compression yes
    TCPKeepAlive yes
    ForwardAgent no
    ForwardX11 no
    ForwardX11Trusted no
    ServerAliveCountMax 30
    ServerAliveInterval 60
    ControlMaster auto
    ControlPath ~/.ssh/%r@%h:%p
    ControlPersist 2000

Host github
    HostName ssh.github.com
    Port 443
    User git

Host remilia
    HostName systemd.club
    User azahi

Host flandre
    HostName flandre.systemd.club
    User azahi

Host sdf
    HostName tty.sdf.org
    User azahi

Host *.42.fr
    SendEnv LANG LC_*
    StrictHostKeyChecking no
    PasswordAuthentication yes

Host 42
    HostName vogsphere.42.fr
    User vogsphere

