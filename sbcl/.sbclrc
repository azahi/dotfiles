;;; -*- mode: Lisp -*-

(let ((gentoo-init "/etc/common-lisp/gentoo-init.lisp"))
  (when (probe-file gentoo-init)
    (load gentoo-init)))

#-quicklisp
(let ((quicklisp-init (merge-pathnames ".quicklisp/setup.lisp"
                                       (user-homedir-pathname))))
