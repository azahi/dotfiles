keystate_numlock = disable
keystate_capslock = disable
keystate_scrolllock= disable

"pkill Xorg"
    Mod4 + Shift + w

"wrapper-terminal"
    Mod4 + Return

"wrapper-terminal -e ncmpcpp"
    Mod4 + Shift + m

"wrapper-terminal -e htop"
    Mod4 + Shift + t

"dmenu_run"
    Mod4 + x

"passmenu"
    Mod4 + Shift + p

"wrapper-browser"
    Mod4 + w

"amixer sset Master toggle"
    XF86AudioMute

"amixer sset Master unmute 5%+"
    XF86AudioRaiseVolume

"amixer sset Master unmute 5%-"
    XF86AudioLowerVolume

"amixer sset Capture toggle"
    XF86AudioMicMute

"amixer sset Capture unmute 5%+"
    Shift + XF86AudioRaiseVolume

"amixer sset Capture unmute 5%-"
    Shift + XF86AudioLowerVolume

"mpc --quiet toggle"
    XF86AudioPlay

"mpc --quiet stop"
    XF86AudioStop

"mpc --quiet next"
    XF86AudioNext

"mpc --quiet prev"
    XF86AudioPrev
